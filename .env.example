# Alibaba Cloud Credentials
# Lấy từ Alibaba Cloud Console: https://ram.console.aliyun.com/manage/ak
ALIBABA_ACCESS_KEY_ID=your-access-key-id-here
ALIBABA_ACCESS_KEY_SECRET=your-access-key-secret-here

# Mock Mode
# Set to true để test với mock API (không cần credentials)
# Set to false để sử dụng Alibaba Cloud API thực tế
MOCK_MODE=true

# Spring Profile
SPRING_PROFILES_ACTIVE=default
